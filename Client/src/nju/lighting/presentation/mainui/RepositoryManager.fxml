<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="680.0" prefWidth="200.0" style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nju.lighting.presentation.mainui.RepositoryManagerController">
    <children>
        <JFXButton fx:id="historyBtn" alignment="CENTER_LEFT" prefHeight="75.0" prefWidth="200.0" text="首页">
            <graphic>
                <ImageView fitHeight="40.0" fitWidth="40.0">
                    <image>
                        <Image url="@../../../../images/个人.png" />
                    </image>
                </ImageView>
            </graphic>
        </JFXButton>
        <JFXButton fx:id="commodityBtn" alignment="CENTER_LEFT" prefHeight="75.0" prefWidth="200.0" text="商品管理">
            <graphic>
                <ImageView fitHeight="40.0" fitWidth="40.0">
                    <image>
                        <Image url="@../../../../images/商品管理.png" />
                    </image>
                </ImageView>
            </graphic>
        </JFXButton>
        <JFXButton fx:id="repositoryViewBtn" alignment="CENTER_LEFT" prefHeight="75.0" prefWidth="200.0" text="库存查看">
            <graphic>
                <ImageView fitHeight="40.0" fitWidth="40.0">
                    <image>
                        <Image url="@../../../../images/库存查看.png" />
                    </image>
                </ImageView>
            </graphic>
        </JFXButton>
        <JFXButton fx:id="repositoryCountingBtn" alignment="CENTER_LEFT" prefHeight="75.0" prefWidth="200.0" text="库存盘点">
            <graphic>
                <ImageView fitHeight="40.0" fitWidth="40.0">
                    <image>
                        <Image url="@../../../../images/库存盘点.png" />
                    </image>
                </ImageView>
            </graphic>
        </JFXButton>
        <JFXButton fx:id="lossAndGainDoc" alignment="CENTER_LEFT" prefHeight="75.0" prefWidth="200.0" text="制定报损报溢单">
            <graphic>
                <ImageView fitHeight="40.0" fitWidth="40.0">
                    <image>
                        <Image url="@../../../../images/报溢单.png" />
                    </image>
                </ImageView>
            </graphic>
        </JFXButton>
        <JFXButton fx:id="alertDoc" alignment="CENTER_LEFT" prefHeight="75.0" prefWidth="200.0" text="制定报警单">
            <graphic>
                <ImageView fitHeight="40.0" fitWidth="40.0">
                    <image>
                        <Image url="@../../../../images/报警单.png" />
                    </image>
                </ImageView>
            </graphic>
        </JFXButton>
      <JFXButton fx:id="giftDoc" alignment="CENTER_LEFT" prefHeight="75.0" prefWidth="200.0" text="制定赠品单">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0">
               <image>
                  <Image url="@../../../../images/报警单.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
    </children>
</VBox>
