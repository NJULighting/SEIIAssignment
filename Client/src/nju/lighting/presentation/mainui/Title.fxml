<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane onMouseDragged="#Drag" onMousePressed="#getOffSet" prefHeight="40.0" prefWidth="1280.0" style="-fx-background-color: #5599FF;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nju.lighting.presentation.mainui.Title">
   <children>
      <Label layoutX="14.0" layoutY="9.0" text="进销存管理平台" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" layoutX="1148.0" prefHeight="40.0" prefWidth="132.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="40.0" style="-fx-background-color: TRANSPARENT, TRANSPARENT, TRANSPARENT, TRANSPARENT;">
               <graphic>
                  <ImageView fx:id="miniBtn" fitHeight="15.0" fitWidth="18.0">
                     <image>
                        <Image url="@../../../../images/最小化_白.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="close" mnemonicParsing="false" onAction="#close" prefHeight="40.0" style="-fx-background-color: TRANSPARENT, TRANSPARENT, TRANSPARENT, TRANSPARENT;">
               <graphic>
                  <ImageView fitHeight="27.0" fitWidth="27.0">
                     <image>
                        <Image url="@../../../../images/退出登录.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button mnemonicParsing="false" prefHeight="40.0" style="-fx-background-color: TRANSPARENT, TRANSPARENT, TRANSPARENT, TRANSPARENT;">
               <graphic>
                  <ImageView fx:id="closeBtn" fitHeight="15.0" fitWidth="15.0">
                     <image>
                        <Image url="@../../../../images/关闭_白.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
   </children>
</Pane>
